{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "YOUR_DOMAIN.onmicrosoft.com",
    "TenantId": "YOUR_TENANT_ID",
    "ClientId": "YOUR_CLIENT_ID",
    "ClientSecret": "YOUR_CLIENT_SECRET",
    "SecretId": "YOUR_SECRET_ID",
    "CallbackPath": "/signin-oidc"
  },
  "Entra": {
    "TenantId": "YOUR_TENANT_ID",
    "ClientId": "YOUR_CLIENT_ID",
    "ClientSecret": "YOUR_CLIENT_SECRET"
  },
  "CmsApi": {
    "BaseUrl": "YOUR_CMS_BASE_URL",
    "ReadApiKey": "YOUR_READ_API_KEY",
    "WriteApiKey": "YOUR_WRITE_API_KEY"
  },
  "SAS": {
    "TenantId": "YOUR_SAS_TENANT_ID",
    "SecretId": "YOUR_SAS_SECRET_ID"
  },
  "EnabledFeatures": {
    "Assurance": false
  },
  "MaintenanceMode": {
    "Enabled": false,
    "HealthCheckIntervalSeconds": 30,
    "HealthCheckTimeoutSeconds": 10
  },
  "Caching": {
    "DefaultDurationMinutes": 5,
    "Redis": {
      "Enabled": true,
      "ConnectionString": "YOUR_AZURE_REDIS_CONNECTION_STRING",
      "Database": 0,
      "KeyPrefix": "fips:"
    },
    "MemoryCache": {
      "SizeLimit": null,
      "CompactionPercentage": 0.25
    },
    "Durations": {
      "Home": 5,
      "Products": 5,
      "ProductDetail": 10,
      "CategoryTypes": 15,
      "CategoryValues": 15,
      "Search": 2,
      "Static": 60,
      "Health": 0.5
    }
  },
  "Airtable": {
    "ApiKey": "YOUR_AIRTABLE_API_KEY",
    "BaseId": "YOUR_AIRTABLE_BASE_ID",
    "FeedbackTableName": "Feedback"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "YOUR_APPLICATION_INSIGHTS_INSTRUMENTATION_KEY",
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING"
  }
}
